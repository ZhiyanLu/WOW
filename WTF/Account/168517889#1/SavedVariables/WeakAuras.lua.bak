						["custom_type"] = "stateupdate",
						["event"] = "Health",
						["names"] = {
						},
						["use_unit"] = true,
						["custom"] = "function(allstates, event, ...)\n    \n    if(event == \"UNIT_SPELLCAST_CHANNEL_START\")then\n        \n        local _,_, spellId = ...\n        \n        --Convoke spellId\n        if(spellId == 323764) then \n            --setup convoke display icon\n            local _, _, icon, startMS, endMS = UnitChannelInfo(\"player\")\n            local duration = (endMS - startMS)/1000\n            local expiration = endMS/1000\n            \n            allstates[\"convoke\"] ={\n                \n                changed = true,\n                show = true,\n                icon = icon,\n                name = \"\",\n                stacks = 0,\n                progressType = \"timed\",\n                duration = duration,\n                expirationTime = expiration,\n                autoHide = false,\n            }        \n            \n            aura_env.startConvoke()\n            \n            return true\n        end\n        \n    end\n    \n    --probably dont need terminated, started and channeling but its easier to keep track where we are during the process of start channeling -> stop channeling -> terminate -> start -> ... and it might be good for conditions in the future\n    --we dont want to listen to anything unless we have started casting convoke and we also dont want to listen to anything, when we terminated the process and also wait for the next cycle\n    if(aura_env.terminated or not aura_env.started) then return false end\n    \n    if(event == \"UNIT_SPELLCAST_CHANNEL_STOP\") then\n        \n        local _,_, spellId = ...\n        \n        --if stopped channeling Convoke\n        if(spellId == 323764) then \n            \n            aura_env.stopConvoke()\n            \n            --playing another animation to show when the waiting for spells is over\n            \n            local state = allstates[\"convoke\"]\n            local duration = aura_env.secondsWaiting\n            \n            state.changed = true\n            state.duration = duration\n            state.expirationTime = GetTime() + duration\n            \n            \n            return true\n        end\n        \n        --We dont need this event anymore\n        return false\n    end\n    \n    if(event == \"COMBAT_LOG_EVENT_UNFILTERED\")then\n        \n        local timeStamp, subEvent, _, _, sourceName, _, _, _, _, _, _, spellId, spellName, _, val, _ = ...\n        \n        \n        --we just want to listen to the spells, that we are the source from\n        if(sourceName ~= UnitName(\"player\")) then return false end\n        \n        aura_env.saveRawData(spellId, subEvent)\n        \n        --get the list of spellIds, that will be registered when combined with subEvent \n        local allowedSpells = aura_env.data[subEvent]\n        \n        --count damage/healing done\n        if(aura_env.config.countDamage)then\n            if(subEvent == \"SPELL_DAMAGE\" or subEvent == \"SPELL_PERIODIC_DAMAGE\")then\n                aura_env.damage = aura_env.damage + val\n            end\n        end\n        \n        if(aura_env.config.countHealing)then\n            if(subEvent == \"SPELL_HEAL\" or subEvent == \"SPELL_PERIODIC_HEAL\")then\n                aura_env.healing = aura_env.healing + val\n            end \n        end\n        \n        --if there are spells to be registered in this subEvent\n        if(allowedSpells) then\n            \n            --for each spellId in list\n            for _, iD in pairs(allowedSpells) do                \n                \n                --spell found\n                if(iD == spellId) then\n                    \n                    --if we cast this spell after the convoke channel, we want to ignore it (time during channeling = false and terminated = true)\n                    if(aura_env.ignoreSpellId[spellId])then return false end\n                    \n                    \n                    -- Fix Spells, that hit Multiple Targets\n                    if(aura_env.lastCast[spellId] == timeStamp) then\n                    return true end\n                    \n                    --We want to ignore spells, that might have been cast after the channel. They should have been taken care of in aura_env.ignoreSpellId but this is another instance of safety to not include too many spells\n                    if(not aura_env.channeling) then \n                        if(not aura_env.isLateSpell(spellId, (aura_env.stopTime-GetTime())))then return true end\n                    end\n                    \n                    --we passed all critera we have for now, so we register this spell\n                    aura_env.registerSpell(spellId, timeStamp, spellName)\n                    \n                    --setup registered spell Display                    \n                    local icon = select(3, GetSpellInfo(spellId))\n                    \n                    if(aura_env.oneIcon)then\n                        local amountCasted = aura_env.castData[spellId][\"amount\"]\n                        if(amountCasted ~= nil and amountCasted >0)then\n                            --look for state with same icon\n                            for _,s in pairs(allstates) do\n                                if(s~=nil and s.icon == icon) then\n                                    s.stacks = amountCasted\n                                    return true\n                                end\n                                \n                            end\n                            \n                        end\n                    end\n                    \n                    allstates[aura_env.count] ={\n                        \n                        changed = true,\n                        name = aura_env.count,\n                        show = true,\n                        icon = icon,\n                        stacks = aura_env.castData[spellId][\"amount\"],\n                        progressType = \"static\",\n                        value = 1,\n                        autoHide = false, \n                        spellId = false,\n                        realId = spellId\n                    }\n                    \n                    return true\n                    \n                end\n            end\n            \n            \n        end\n        return true        \n    end    \n    \n    --Terminate Convoke cycle\n    if(event == \"OmnyTerminateConvoke\") then\n        \n        aura_env.terminate()\n        \n        --hide every icon\n        for _,state in pairs(allstates) do\n            \n            \n            state.changed = true\n            state.show = false\n        end\n        \n        return true\n    end\n    \n    --Listen for spells that have been cast after channeling (convoke spells do not trigger this, only if you hit the spells yourself)\n    if(event == \"UNIT_SPELLCAST_SUCCEEDED\")then\n        \n        local _,_, spellId = ...\n        \n        --probably we dont need that check for convoke but there is also no point to save it in ignoreSpellId cause you wont be able to hit it yourself in a channel start -> channel stop -> terminate cycle\n        if(spellId ~= 323764) then \n            \n            aura_env.ignoreSpellId[spellId] = true\n            \n            --fixing moonfire -> spellId 8921 cast but on CLEU SPELL_DAMAGE event it shows up as 164812\n            if(spellId == 8921) then\n                aura_env.ignoreSpellId[164812] = true\n            end\n            \n            \n        end\n        \n    end\n    \nend\n\n\n",
