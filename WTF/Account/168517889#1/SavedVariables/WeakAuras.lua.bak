			["actions"] = {
				["start"] = {
				},
				["init"] = {
					["custom"] = "local e = aura_env\ne.rewardProgress = {}\ne.history = {}\ne.torghastState = {}\n\nlocal i18n = {}\nlocal locale = GetLocale()\nlocal function L(text)\n    if i18n[locale] then return i18n[locale][text] or text end\n    return text\nend\n\nlocal moduleEnable ={\n    [\"MythicPlus\"] = e.config.moduleEnable[1],\n    [\"PvP\"] = e.config.moduleEnable[2],\n    [\"Raid\"] = e.config.moduleEnable[3],\n    [\"Torghast\"] = e.config.moduleEnable[4],\n    [\"MythicPlusList\"] = e.config.moduleEnable[5],\n} \n\nfunction e:GetModuleEnableState(typestr)\n    if type(typestr) ==\"number\" then\n        if typestr == Enum.WeeklyRewardChestThresholdType.Raid then\n            return moduleEnable[\"Raid\"]\n        elseif typestr == Enum.WeeklyRewardChestThresholdType.MythicPlus then\n            return moduleEnable[\"MythicPlus\"]\n        elseif typestr == Enum.WeeklyRewardChestThresholdType.RankedPvP then\n            return moduleEnable[\"PvP\"]\n        end  \n    end\n    return moduleEnable[typestr] or false\nend\n\n-- 对本地缓存的表进行维护\nfunction e:UpdateActivity()\n    local activityInfo =  C_WeeklyRewards.GetActivities()\n    local text,item\n    for _,v in pairs(activityInfo) do \n        if not self.rewardProgress[v.type] then self.rewardProgress[v.type] = {} end \n        text = self:GetRewardText(v.type,v.level) or L(\"未确定\")\n        self.rewardProgress[v.type][v.index] = {\n            id = v.id,\n            level = v.level,\n            difficulty = text,\n            progress = v.progress,\n            threshold = v.threshold,\n            unlocked = v.progress>=v.threshold,\n        }\n        if self.rewardProgress[v.type][v.index].unlocked then \n            self.rewardProgress[v.type][v.index].item = self:GetRewardItemInfo(v.id) or \"\"\n        end\n    end\nend\n\nlocal comparison = function(entry1, entry2)\n    if ( entry1.level == entry2.level ) then\n        return entry1.mapChallengeModeID < entry2.mapChallengeModeID;\n    else\n        return entry1.level > entry2.level;\n    end\nend\n-- 对本地缓存的大秘境历史记录进行更新\nfunction e:UpdateHistory()\n    local runInfo = C_MythicPlus.GetRunHistory()\n    table.sort(runInfo,comparison)\n    if #runInfo >0 then\n        local text\n        wipe(self.history)\n        for _,v in pairs(runInfo) do \n            if v.thisWeek then\n                text = C_ChallengeMode.GetMapUIInfo(v.mapChallengeModeID);    \n                v.text = v.level ..\" \"..text\n                if not v.completed then\n                    v.text = v.text .. L(\"未完成\")    \n                end\n                table.insert(self.history,v)\n            end\n        end \n    end\n    --table.sort(self.history,comparison)\nend\nfunction e:UpdateTorghast()\n    local sets = C_UIWidgetManager.GetAllWidgetsBySetID(399)\n    wipe(self.torghastState)\n    for _,v in ipairs(sets) do \n        local widgetInfo = C_UIWidgetManager.GetTextWithStateWidgetVisualizationInfo(v.widgetID) \n        \n        if widgetInfo and widgetInfo.shownState == Enum.WidgetShownState.Shown then\n            self.torghastState[widgetInfo.orderIndex] = widgetInfo.text\n        end\n    end\n    \n    for i,v in pairs(self.torghastState) do \n        if i%2 ==0 then\n            self.torghastState[i] = string.format(\"%s \\n|r%s|r\",v,self.torghastState[i+1])\n            self.torghastState[i+1] = nil\n        end\n    end\n    \nend\n\n\nfunction e:UpdateData()\n    self:UpdateActivity()\n    self:UpdateHistory()\nend\n\n-- 对特定ID的奖励物品等级进行更新\nfunction e:UpdateRewardItemLevel(id,item)\n    for type,rewards in pairs(self.rewardProgress) do \n        for _,reward in pairs(rewards) do \n            if  reward.id == id then\n                reward.item = item\n                return    \n            end\n        end\n    end\nend\n\n-- 尝试获取特定ID的奖励物品等级 如果获取不到就每秒检查一次直到获取\nfunction e:GetRewardItemInfo(id)\n    local itemLevel\n    local itemLink = C_WeeklyRewards.GetExampleRewardItemHyperlinks(id);\n    if itemLink then\n        itemLevel =  GetDetailedItemLevelInfo(itemLink);\n        if not itemLevel then C_Timer.After(1,function() WeakAuras.ScanEvents(\"WA_UPDATE_REWARDS\",id) end) end\n        return itemLevel\n    end    \nend\n-- 获取进度种类\nfunction e:GetRewardTypeHead(type)\n    if type == Enum.WeeklyRewardChestThresholdType.Raid then\n        return  RAIDS\n    elseif type == Enum.WeeklyRewardChestThresholdType.MythicPlus then\n        return MYTHIC_DUNGEONS\n    elseif type == Enum.WeeklyRewardChestThresholdType.RankedPvP then\n        return PVP\n    end\nend\n\nfunction e:GetWeeklyProgressText(index,reward)\n    return string.format(L(\"阶段%s %s %s\"),index,reward.difficulty,e:GetRewardProgressText(reward.unlocked,reward.progress,reward.threshold,reward.item))\nend\n\n-- 根据进度来更新文字和奖励物品装等显示\nfunction e:GetRewardProgressText(unlocked,progress,threshold,item)\n    if unlocked then \n        if not item then\n            return string.format(L(\"已解锁 - %s\"),RETRIEVING_ITEM_INFO) \n        else\n            return string.format(L(\"已解锁 - %s 装等\"),item) \n        end\n    end\n    return progress..\"/\"..threshold\nend\n\n-- 获取基于何种等级的物品奖励名称\nfunction e:GetRewardText(type,level)\n    if type == Enum.WeeklyRewardChestThresholdType.Raid then\n        return  DifficultyUtil.GetDifficultyName(level);\n    elseif type == Enum.WeeklyRewardChestThresholdType.MythicPlus then\n        if level == 0 then return L(\"未确定\") end\n        return string.format(WEEKLY_REWARDS_MYTHIC, level);\n    elseif type == Enum.WeeklyRewardChestThresholdType.RankedPvP then\n        return PVPUtil.GetTierName(level);\n    end\nend\n-- 本周是否有大米历史记录（已完成\nfunction e:HasHistory()\n    return   #self.history >0\nend\n\n-- 对大米历史记录（已完成）进行字符串拼接\nfunction e:GetHistoryText()\n    local result = L(\"最佳大米记录(已完成)\")..\":\"\n    local maxlength = #self.history\n    if maxlength >= self.config.mythicnum then maxlength = self.config.mythicnum end \n    for i=1,maxlength,1 do \n        result = result ..\" \\n\"..self.history[i].text    \n    end\n    return result\nend\n\nfunction e:RegisterI18n(locale,tbl)\n    i18n[locale] = tbl\nend\n\nlocal compatibility = WeakAuras.GetRegion(e.id).compatibility or {}\nWeakAuras.GetRegion(e.id).compatibility = compatibility\n\nfunction e.RegisterCompatibility(frame,xoffset,yoffset)\n    compatibility[frame] = {x=xoffset,y=yoffset}\nend\n\nlocal function sendCheck(state,frame,offset)\n    WeakAuras.ScanEvents(\"WA_COMPATIBILITY_CHECK\",state,frame,offset)    \nend\n\nfunction e.InitCompatibilitys()\n    e.parent = PVEFrame\n    local onShow = function(self) sendCheck(\"OnShow\",self,compatibility[self]) end\n    local onHide = function(self) sendCheck(\"OnHide\",self,compatibility[self]) end\n    for frame,offset in pairs(compatibility) do \n        if frame and not frame.wahooked then\n            frame:HookScript(\"OnShow\",onShow)\n            frame:HookScript(\"OnHide\",onHide)\n            frame.wahooked = true\n            print(frame:GetName(),\"Register\")\n        end\n    end\nend\n\ne.RegisterCompatibility(PremadeGroupsFilterDialog,PremadeGroupsFilterDialog:GetWidth(),0)\n\nWeakAuras.ScanEvents(\"WA_REWARDS_STATE_INIT\")\n--提前在客户端缓存相关数据\nC_MythicPlus.RequestMapInfo()\n-- 初次更新\ne:UpdateData()\ne:UpdateTorghast()\n\ne.InitCompatibilitys()",
