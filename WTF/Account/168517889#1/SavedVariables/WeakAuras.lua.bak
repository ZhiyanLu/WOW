					["custom"] = "if aura_env then\n    \n    -- set region (for ease)\n    local bar = WeakAuras.regions[aura_env.id].region.bar\n    \n    if aura_env.state and aura_env.state.duration then\n        \n        -- get the max time the bar is showing\n        local max = aura_env.state.duration\n        \n        -- get the bar width/Height\n        local maxSize = WeakAuras.regions[aura_env.id].region.bar:GetWidth()\n        \n        local size = maxSize/max\n        \n        local offset = { \n            size * (15-5),\n            size * (15-10),\n        }\n        \n        \n        for i=1,15,1 do\n            bar[\"spark\"..i]:SetPoint(\n                \"LEFT\", \n                WeakAuras.regions[aura_env.id].region.bar,\n                \"LEFT\", \n                offset[i], \n                0\n            )\n        end\n    end\nend",
					["do_sound"] = true,
